<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MyHorizontalView"></declare-styleable>
    <declare-styleable name="IsmartvVideoView"></declare-styleable>
    <declare-styleable name="AsyncImageView">
        <attr name="defaultSrc" format="reference" />
        <attr name="url" format="string" />
        <attr name="inDensity">
            <enum name="ldpi" value="120" />
            <enum name="mdpi" value="160" />
            <enum name="hdpi" value="240" />
            <enum name="xhdpi" value="320" />
            <enum name="xxhdpi" value="480"></enum>
        </attr>
    </declare-styleable>
    <declare-styleable name="HGridView">
        <attr name="horizontalSpacing" format="dimension" />
        <attr name="verticalSpacing" format="dimension" />
        <attr name="rowHeight" format="dimension" />
        <attr name="numRows" format="integer" />
        <attr name="labelDrawable" format="integer" />
        <attr name="labelBackgroundDrawable" format="integer" />
        <attr name="selectorDrawable" format="integer" />
        <attr name="labelLeftOffset" format="dimension" />
        <attr name="labelTopOffset" format="dimension" />
        <attr name="labelWidth" format="dimension" />
        <attr name="labelHeight" format="dimension" />
        <attr name="labelBackgroundLeftOffset" format="dimension" />
        <attr name="labelBackgroundTopOffset" format="dimension" />
        <attr name="labelBackgroundWidth" format="dimension" />
        <attr name="labelBackgroundHeight" format="dimension" />
        <attr name="labelTextSize" format="dimension" />
        <attr name="labelTextColor" format="color" />
        <attr name="labelTextMarginTop" format="dimension" />
        <attr name="labelTextMarginLeft" format="dimension" />
        <attr name="labelTextMarginRight" format="dimension" />
        <attr name="labelTextMarginBottom" format="dimension" />
        <attr name="selectionLeftPadding" format="dimension" />
        <attr name="selectionTopPadding" format="dimension" />
        <attr name="selectionRightPadding" format="dimension" />
        <attr name="selectionBottomPadding" format="dimension" />
    </declare-styleable>
    <declare-styleable name="ZGridView">

        <!-- Defines the default horizontal spacing between columns. -->
        <attr name="horizontalSpace" format="dimension" />
        <!-- Defines the default vertical spacing between rows. -->
        <attr name="verticalSpace" format="dimension" />
        <!-- Defines how columns should stretch to fill the available empty space, if any. -->
        <attr name="stretchMode">

            <!-- Stretching is disabled. -->
            <enum name="none" value="0" />
            <!-- The spacing between each column is stretched. -->
            <enum name="spacingWidth" value="1" />
            <!-- Each column is stretched equally. -->
            <enum name="columnWidth" value="2" />
            <!-- The spacing between each column is uniformly stretched.. -->
            <enum name="spacingWidthUniform" value="3" />
        </attr>
        <!-- Specifies the fixed width for each column. -->
        <attr name="columnWidth" format="dimension" />
        <!-- Defines how many columns to show. -->
        <attr name="numColumns" format="integer" min="0">

            <!-- Display as many columns as possible to fill the available space. -->
            <enum name="auto_fit" value="-1" />
        </attr>
        <!-- Specifies the gravity within each cell. -->
        <attr name="gravity" format="integer" />
        <!-- //////////////////////////////////////////////////////////// -->
        <!-- Drawable used to indicate the currently selected item in the list. -->
        <attr name="listSelector" format="color|reference" />
        <attr name="drawSelectorOnTop" format="boolean" />
        <attr name="stackFromBottom" format="boolean" />
        <attr name="scrollingCache" format="boolean" />
        <attr name="textFilterEnabled" format="boolean" />
        <!--
             Sets the transcript mode for the list. In transcript mode, the list
             scrolls to the bottom to make new items visible when they are added.
        -->
        <attr name="transcriptMode">
            <enum name="disabled" value="0" />
            <enum name="normal" value="1" />
            <enum name="alwaysScroll" value="2" />
        </attr>
        <attr name="cacheColorHint" format="color" />
        <attr name="fastScrollEnabled" format="boolean" />
        <attr name="smoothScrollbar" format="boolean" />
        <attr name="choiceMode">

            <!-- Normal list that does not indicate choices. -->
            <enum name="none" value="0" />
            <!-- The list allows up to one choice. -->
            <enum name="singleChoice" value="1" />
            <!-- The list allows multiple choices. -->
            <enum name="multipleChoice" value="2" />
            <!-- The list allows multiple choices in a custom selection mode. -->
            <enum name="multipleChoiceModal" value="3" />
        </attr>

        <!--
             When set to true, the list will always show the fast scroll interface.
             This setting implies fastScrollEnabled.
        -->
        <attr name="fastScrollAlwaysVisible" format="boolean" />
        <attr name="scaleSize" format="float" />
        <attr name="selectorLeftPadding" format="dimension" />
        <attr name="selectorTopPadding" format="dimension" />
        <attr name="selectorRightPadding" format="dimension" />
        <attr name="selectorBottomPadding" format="dimension" />
    </declare-styleable>


    <declare-styleable name="VerticalGuideListView"></declare-styleable>

    <declare-styleable name="HorizontalGuideListView"></declare-styleable>

    <declare-styleable name="ChannelGridView"></declare-styleable>

    <declare-styleable name="ContainerLayout">

    </declare-styleable>

    <declare-styleable name="LoginPanelView">
        <attr name="suspension_window" format="boolean" />
    </declare-styleable>


    <declare-styleable name="MarqueeView">
        <attr name="textsize" format="dimension" />
        <attr name="textcolor" format="reference|color" />
        <attr name="speed" format="integer"></attr>
    </declare-styleable>


    <declare-styleable name="SakuraImageView"></declare-styleable>

    <declare-styleable name="IconPagerIndicator"></declare-styleable>

    <declare-styleable name="IconImageView">

    </declare-styleable>

    <declare-styleable name="SakuraButton"></declare-styleable>
    <declare-styleable name="SakuraListView"></declare-styleable>
    <declare-styleable name="SakuraSpinner"></declare-styleable>
    <declare-styleable name="SakuraEditText"></declare-styleable>
    <declare-styleable name="MessageSubmitButton"></declare-styleable>
    <declare-styleable name="FeedBackListView"></declare-styleable>


    <!-- new  guide activity -->
    <declare-styleable name="HorizontalListView"></declare-styleable>

    <declare-styleable name="DaisyViewContainer">
        <attr name="horizontal_spacing" format="dimension" />
        <attr name="vertical_spacing" format="dimension" />
        <attr name="item_height" format="dimension" />
        <attr name="item_width" format="dimension" />
    </declare-styleable>

    <declare-styleable name="DaisyVideoView" />


</resources>