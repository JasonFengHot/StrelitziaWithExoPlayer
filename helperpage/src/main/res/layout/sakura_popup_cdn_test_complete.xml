<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:gravity="center"
              android:layout_height="match_parent">

    <TextView
            android:id="@+id/complete_title"
            android:layout_marginTop="20dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="24sp"
            android:textColor="#ff6b6b6b"
            android:gravity="center"
            />

    <tv.ismar.sakura.ui.widget.SakuraButton
            android:id="@+id/test_c_confirm_btn"
            android:text="@string/confirm_test_complete"
            android:textSize="22sp"
            android:gravity="center"
            android:textColor="#ffffffff"
            android:layout_marginTop="20dp"
            android:background="@drawable/sakura_selector_button"
            android:layout_width="120dp"
            android:layout_height="60dp"/>

</LinearLayout>